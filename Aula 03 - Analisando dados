from datatime import date
atual = date.today().yaer
maior = 0
menos = 0
 for pessoas in range (1,10)
  nasc = int(input('digite a {}° data de nascimento'.format(pessas))
  if atual >= 21:
   maior +=1
   else: 
   menor +=1
 print ('Das idades apresentadas,{} são maiores e {} são menores de idade!'.format(maior,menor))
