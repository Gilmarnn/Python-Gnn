maiorpeso = 0
menorpeso = 0
for p in range (1,10):
  peso = float('Digite o {}º peso: '))
   if p == 1:
   maiorpeso = peso
   menorpeso = peso
   else:
    if peso > maiorpeso:
     maiorpeso = peso
      if  peso < menorpeso:
       menorpeso = peso
       
   print ('Dos pesos ditados o maior é {}kg e o menor é {}kg'.format(maiorpeso,menopeso)
       
   
